<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Refresh" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask TaskName="DependencyStore.Tasks.DsRefresh" AssemblyFile="Build\Debug\DependencyStore.dll" />

  <PropertyGroup>
    <DryRun Condition="'$(DryRun)'==''">True</DryRun>
  </PropertyGroup>

  <Target Name="Refresh">
    <DsRefresh ProjectName="DependencyStore" DryRun="$(DryRun)" />
  </Target>
</Project>